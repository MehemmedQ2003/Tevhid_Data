    {
      "id": "6232",
      "suraNo": "114",
      "suraName": "Nâs",
      "aya": "5",
      "arabic_text": "اَلَّذ۪ي يُوَسْوِسُ ف۪ي صُدُورِ النَّاسِۙ",
      "translation": "“Ki o, insanların göğüslerine vesvese verip durur.”",
      "footnotes": null
    },
    {
      "id": "6233",
      "suraNo": "114",
      "suraName": "Nâs",
      "aya": "6",
      "arabic_text": "مِنَ الْجِنَّةِ وَالنَّاسِ",
      "translation": "“Cinlerden ve insanlardan olan.”",
      "footnotes": null
    }
