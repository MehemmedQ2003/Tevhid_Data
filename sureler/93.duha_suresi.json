{
  "result": [
    {
      "id": "6077",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "1",
      "arabic_text": "وَالضُّحٰىۙ",
      "translation": "Andolsun kuşluk vaktine,",
      "footnotes": null
    },
    {
      "id": "6078",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "2",
      "arabic_text": "وَالَّيْلِ اِذَا سَجٰىۙ",
      "translation": "Ve örttüğünde geceye.",
      "footnotes": null
    },
    {
      "id": "6079",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "3",
      "arabic_text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلٰىۜ",
      "translation": "Rabbin seni terk etmedi ve sana darılmadı.",
      "footnotes": null
    },
    {
      "id": "6080",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "4",
      "arabic_text": "وَلَلْاٰخِرَةُ خَيْرٌ لَكَ مِنَ الْاُو۫لٰىۜ",
      "translation": "Kuşkusuz senin için ahiret, dünyadan daha hayırlıdır.",
      "footnotes": null
    },
    {
      "id": "6081",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "5",
      "arabic_text": "وَلَسَوْفَ يُعْط۪يكَ رَبُّكَ فَتَرْضٰىۜ",
      "translation": "Elbette Rabbin, sana verecek ve sen de razı olacaksın.",
      "footnotes": null
    },
    {
      "id": "6082",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "6",
      "arabic_text": "اَلَمْ يَجِدْكَ يَت۪يمًا فَاٰوٰىۖ",
      "translation": "Sen bir yetimken seni bulup barındırmadı mı?",
      "footnotes": null
    },
    {
      "id": "6083",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "7",
      "arabic_text": "وَوَجَدَكَ ضَٓالًّا فَهَدٰىۖ",
      "translation": "Sen (Kitab’ı ve imanı) bilmezken sana doğru yolu göstermedi mi?",
      "footnotes": null
    },
    {
      "id": "6084",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "8",
      "arabic_text": "وَوَجَدَكَ عَٓائِلًا فَاَغْنٰىۜ",
      "translation": "Bir fakirken seni zengin kılmadı mı?",
      "footnotes": null
    },
    {
      "id": "6085",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "9",
      "arabic_text": "فَاَمَّا الْيَت۪يمَ فَلَا تَقْهَرْۜ",
      "translation": "O hâlde, sakın yetimi kahretme/hor görüp ezme!",
      "footnotes": null
    },
    {
      "id": "6086",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "10",
      "arabic_text": "وَاَمَّا السَّٓائِلَ فَلَا تَنْهَرْۜ",
      "translation": "Ve sakın isteyeni azarlayıp tersleme!",
      "footnotes": null
    },
    {
      "id": "6087",
      "suraNo": "93",
      "suraName": "Duhâ",
      "aya": "11",
      "arabic_text": "وَاَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ",
      "translation": "Ve Rabbinin nimetini anlat.",
      "footnotes": null
    }
  ]
}