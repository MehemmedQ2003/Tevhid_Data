  "result": [
    {
      "id": "6096",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "1",
      "arabic_text": "وَالتّ۪ينِ وَالزَّيْتُونِۙ",
      "translation": "Andolsun incire ve zeytine,",
      "footnotes": null
    },
    {
      "id": "6097",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "2",
      "arabic_text": "وَطُورِ س۪ين۪ينَۙ",
      "translation": "Sina’daki Tur Dağı’na,",
      "footnotes": null
    },
    {
      "id": "6098",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "3",
      "arabic_text": "وَهٰذَا الْبَلَدِ الْاَم۪ينِۙ",
      "translation": "Ve bu güvenli beldeye.",
      "footnotes": null
    },
    {
      "id": "6099",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "4",
      "arabic_text": "لَقَدْ خَلَقْنَا الْاِنْسَانَ ف۪ٓي اَحْسَنِ تَقْو۪يمٍۘ",
      "translation": "Andolsun ki, insanı en güzel surette yarattık.",
      "footnotes": null
    },
    {
      "id": "6100",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "5",
      "arabic_text": "ثُمَّ رَدَدْنَاهُ اَسْفَلَ سَافِل۪ينَۙ",
      "translation": "Sonra onu esfel-i safiline (aşağıların aşağısına) çevirdik.",
      "footnotes": null
    },
    {
      "id": "6101",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "6",
      "arabic_text": "اِلَّا الَّذ۪ينَ اٰمَنُوا وَعَمِلُوا الصَّالِحَاتِ فَلَهُمْ اَجْرٌ غَيْرُ مَمْنُونٍۜ",
      "translation": "İman edip salih amel işleyenler müstesna. Onlar için kesintisiz bir mükâfat vardır.",
      "footnotes": null
    },
    {
      "id": "6102",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "7",
      "arabic_text": "فَمَا يُكَذِّبُكَ بَعْدُ بِالدّ۪ينِۜ",
      "translation": "(Ey insan!) Bundan sonra dini/hesabı sana yalanlatan şey nedir?",
      "footnotes": null
    },
    {
      "id": "6103",
      "suraNo": "95",
      "suraName": "Tîn",
      "aya": "8",
      "arabic_text": "اَلَيْسَ اللّٰهُ بِاَحْكَمِ الْحَاكِم۪ينَ",
      "translation": "Allah, hükmedenlerin en hâkimi/en güzel hükmeden değil mi?",
      "footnotes": null
    }
  ]
}