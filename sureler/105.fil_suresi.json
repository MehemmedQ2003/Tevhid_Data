  "result": [
    {
      "id": "6186",
      "suraNo": "105",
      "suraName": "Fîl",
      "aya": "1",
      "arabic_text": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِاَصْحَابِ الْف۪يلِۜ",
      "translation": "Rabbinin fil sahiplerine yaptığını görmedin mi?",
      "footnotes": null
    },
    {
      "id": "6187",
      "suraNo": "105",
      "suraName": "Fîl",
      "aya": "2",
      "arabic_text": "اَلَمْ يَجْعَلْ كَيْدَهُمْ ف۪ي تَضْل۪يلٍۙ",
      "translation": "Onların (Kâbe’yi yıkmak için) kurdukları düzeni boşa çıkarmadı mı?",
      "footnotes": null
    },
    {
      "id": "6188",
      "suraNo": "105",
      "suraName": "Fîl",
      "aya": "3",
      "arabic_text": "وَاَرْسَلَ عَلَيْهِمْ طَيْرًا اَبَاب۪يلَۙ",
      "translation": "Üzerlerine Ebabil kuşlarını gönderdi.",
      "footnotes": null
    },
    {
      "id": "6189",
      "suraNo": "105",
      "suraName": "Fîl",
      "aya": "4",
      "arabic_text": "تَرْم۪يهِمْ بِحِجَارَةٍ مِنْ سِجّ۪يلٍۖۙ",
      "translation": "Onlara pişirilmiş çamurdan taşlar atıyorlardı.",
      "footnotes": null
    },
    {
      "id": "6190",
      "suraNo": "105",
      "suraName": "Fîl",
      "aya": "5",
      "arabic_text": "فَجَعَلَهُمْ كَعَصْفٍ مَأْكُولٍ",
      "translation": "Onları yenilmiş ekin yaprağı gibi (darmadağın) etti.",
      "footnotes": null
    }
  ]
}