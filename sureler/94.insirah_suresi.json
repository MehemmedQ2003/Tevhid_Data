  "result": [
    {
      "id": "6088",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "1",
      "arabic_text": "اَلَمْ نَشْرَحْ لَكَ صَدْرَكَۙ",
      "translation": "Biz göğsünü genişletip (hidayete ve hayra elverişli hâle) getirmedik mi?",
      "footnotes": null
    },
    {
      "id": "6089",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "2",
      "arabic_text": "وَوَضَعْنَا عَنْكَ وِزْرَكَۙ",
      "translation": "(Nübüvvet öncesi hatalarını veya nübüvvetin ağır) yükünü üzerinden indirmedik mi?",
      "footnotes": null
    },
    {
      "id": "6090",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "3",
      "arabic_text": "اَلَّذ۪ٓي اَنْقَضَ ظَهْرَكَۙ",
      "translation": "Ki o yük, senin belini bükmüştü.",
      "footnotes": null
    },
    {
      "id": "6091",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "4",
      "arabic_text": "وَرَفَعْنَا لَكَ ذِكْرَكَۜ",
      "translation": "Senin şanını yüceltmedik mi?",
      "footnotes": null
    },
    {
      "id": "6092",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "5",
      "arabic_text": "فَاِنَّ مَعَ الْعُسْرِ يُسْرًاۙ",
      "translation": "Hiç şüphesiz, zorlukla beraber kolaylık vardır.",
      "footnotes": null
    },
    {
      "id": "6093",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "6",
      "arabic_text": "اِنَّ مَعَ الْعُسْرِ يُسْرًاۜ",
      "translation": "(Evet,) zorlukla beraber kolaylık vardır.",
      "footnotes": null
    },
    {
      "id": "6094",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "7",
      "arabic_text": "فَاِذَا فَرَغْتَ فَانْصَبْۙ",
      "translation": "(Öyleyse) boş kaldığında hemen (ibadet ve taate koyul ve) yorul.",
      "footnotes": null
    },
    {
      "id": "6095",
      "suraNo": "94",
      "suraName": "İnşirâh",
      "aya": "8",
      "arabic_text": "وَاِلٰى رَبِّكَ فَارْغَبْ",
      "translation": "Ve yalnızca Rabbine rağbet et/yönel.",
      "footnotes": null
    }
  ]
}