{
  "result": [
    {
      "id": "6195",
      "suraNo": "107",
      "suraName": "Maûn",
      "aya": "1",
      "arabic_text": "اَرَاَيْتَ الَّذ۪ي يُكَذِّبُ بِالدّ۪ينِۜ",
      "translation": "Dinin (hükümlerini) yalanlayanı gördün mü?",
      "footnotes": null
    },
    {
      "id": "6196",
      "suraNo": "107",
      "suraName": "Maûn",
      "aya": "2",
      "arabic_text": "فَذٰلِكَ الَّذ۪ي يَدُعُّ الْيَت۪يمَۙ",
      "translation": "İşte yetimi itip kovan budur.",
      "footnotes": null
    },
    {
      "id": "6197",
      "suraNo": "107",
      "suraName": "Maûn",
      "aya": "3",
      "arabic_text": "وَلَا يَحُضُّ عَلٰى طَعَامِ الْمِسْك۪ينِۜ",
      "translation": "Yoksulu da doyurmaya teşvik etmez.",
      "footnotes": null
    },
    {
      "id": "6198",
      "suraNo": "107",
      "suraName": "Maûn",
      "aya": "4",
      "arabic_text": "فَوَيْلٌ لِلْمُصَلّ۪ينَۙ",
      "translation": "Veyl olsun o namaz kılanlara!",
      "footnotes": null
    },
    {
      "id": "6199",
      "suraNo": "107",
      "suraName": "Maûn",
      "aya": "5",
      "arabic_text": "اَلَّذ۪ينَ هُمْ عَنْ صَلَاتِهِمْ سَاهُونَۙ",
      "translation": "Onlar ki namazlarında gaflet içindelerdir.",
      "footnotes": null
    },
    {
      "id": "6200",
      "suraNo": "107",
      "suraName": "Maûn",
      "aya": "6",
      "arabic_text": "اَلَّذ۪ينَ هُمْ يُرَٓاؤُ۫نَۙ",
      "translation": "Onlar, riyakâr kimselerdir.",
      "footnotes": null
    },
    {
      "id": "6201",
      "suraNo": "107",
      "suraName": "Maûn",
      "aya": "7",
      "arabic_text": "وَيَمْنَعُونَ الْمَاعُونَ",
      "translation": "Ve onlar, insanların gündelik yardımlaşmalarına dahi engel olurlar.",
      "footnotes": null
    }
  ]
}