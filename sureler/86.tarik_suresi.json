{
  "result": [
    {
      "id": "5929",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "1",
      "arabic_text": "وَالسَّمَٓاءِ وَالطَّارِقِۙ",
      "translation": "Semaya ve Tarık’a (gece görünen yıldıza) andolsun.",
      "footnotes": null
    },
    {
      "id": "5930",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "2",
      "arabic_text": "وَمَٓا اَدْرٰيكَ مَا الطَّارِقُۙ",
      "translation": "Sen, Tarık’ın ne olduğunu nereden bileceksin?",
      "footnotes": null
    },
    {
      "id": "5931",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "3",
      "arabic_text": "اَلنَّجْمُ الثَّاقِبُۙ",
      "translation": "O delip geçen/aydınlatan yıldızdır.",
      "footnotes": null
    },
    {
      "id": "5932",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "4",
      "arabic_text": "اِنْ كُلُّ نَفْسٍ لَمَّا عَلَيْهَا حَافِظٌۜ",
      "translation": "Her nefsin üzerinde mutlaka bir koruyucu/kaydeden gözetleyici vardır.",
      "footnotes": null
    },
    {
      "id": "5933",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "5",
      "arabic_text": "فَلْيَنْظُرِ الْاِنْسَانُ مِمَّ خُلِقَۜ",
      "translation": "(Öyleyse) insan neden yaratıldığına bir baksın?",
      "footnotes": null
    },
    {
      "id": "5934",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "6",
      "arabic_text": "خُلِقَ مِنْ مَٓاءٍ دَافِقٍۙ",
      "translation": "Tazyikle atılan bir sudan yaratıldı.",
      "footnotes": null
    },
    {
      "id": "5935",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "7",
      "arabic_text": "يَخْرُجُ مِنْ بَيْنِ الصُّلْبِ وَالتَّرَٓائِبِۜ",
      "translation": "(O,) bel kemiği ile omurga arasından çıkar.",
      "footnotes": null
    },
    {
      "id": "5936",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "8",
      "arabic_text": "اِنَّهُ عَلٰى رَجْعِه۪ لَقَادِرٌۜ",
      "translation": "Hiç şüphesiz (Allah), onu döndürmeye (öldükten sonra diriltmeye) kâdirdir.",
      "footnotes": null
    },
    {
      "id": "5937",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "9",
      "arabic_text": "يَوْمَ تُبْلَى السَّرَٓائِرُۙ",
      "translation": "O gün, tüm sırlar açığa çıkacaktır.",
      "footnotes": null
    },
    {
      "id": "5938",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "10",
      "arabic_text": "فَمَا لَهُ مِنْ قُوَّةٍ وَلَا نَاصِرٍۜ",
      "translation": "(İnsanın) hiçbir gücü ve yardımcısı olmayacaktır.",
      "footnotes": null
    },
    {
      "id": "5939",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "11",
      "arabic_text": "وَالسَّمَٓاءِ ذَاتِ الرَّجْعِۙ",
      "translation": "Dönüşlü (yağmurları sürekli tekrar eden) göğe andolsun.",
      "footnotes": null
    },
    {
      "id": "5940",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "12",
      "arabic_text": "وَالْاَرْضِ ذَاتِ الصَّدْعِۙ",
      "translation": "(Bitkilerin yardığı) yarıklı yeryüzüne andolsun.",
      "footnotes": null
    },
    {
      "id": "5941",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "13",
      "arabic_text": "اِنَّهُ لَقَوْلٌ فَصْلٌۙ",
      "translation": "Hiç şüphesiz ki o (Kur’ân), ayırt edici/son sözü söyleyendir.",
      "footnotes": null
    },
    {
      "id": "5942",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "14",
      "arabic_text": "وَمَا هُوَ بِالْهَزْلِۜ",
      "translation": "O ciddiyetsiz, şaka olan (bir söz) değildir.",
      "footnotes": null
    },
    {
      "id": "5943",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "15",
      "arabic_text": "اِنَّهُمْ يَك۪يدُونَ كَيْدًاۙ",
      "translation": "Hiç kuşkusuz, onlar tuzak kuruyorlar.",
      "footnotes": null
    },
    {
      "id": "5944",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "16",
      "arabic_text": "وَاَك۪يدُ كَيْدًاۚ",
      "translation": "Ben de tuzak kuruyorum.",
      "footnotes": null
    },
    {
      "id": "5945",
      "suraNo": "86",
      "suraName": "Târık",
      "aya": "17",
      "arabic_text": "فَمَهِّلِ الْكَافِر۪ينَ اَمْهِلْهُمْ رُوَيْدًا",
      "translation": "Kâfirlere mühlet tanı. (Çok değil) az bir mühlet.",
      "footnotes": null
    }
  ]
}